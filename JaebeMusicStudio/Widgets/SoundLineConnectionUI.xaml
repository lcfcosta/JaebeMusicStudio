<UserControl x:Class="JaebeMusicStudio.Widgets.SoundLineConnectionUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JaebeMusicStudio.Widgets"
             xmlns:UI="clr-namespace:JaebeMusicStudio.UI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Name="name" Grid.ColumnSpan="1"></Label>
        <UI:Slider Grid.Column="1" x:Name="volume" ValueChanged="volume_ValueChanged"></UI:Slider>
    </Grid>
</UserControl>
