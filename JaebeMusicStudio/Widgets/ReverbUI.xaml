<Page x:Class="JaebeMusicStudio.Widgets.ReverbUI"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JaebeMusicStudio.Widgets"
      xmlns:UI="clr-namespace:JaebeMusicStudio.UI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="NonlinearDistortionUI">

    <Grid>
        <UI:Slider VerticalAlignment="Top" Margin="10,0" x:Name="Volume" ValueChanged="Volume_OnValueChanged" Minimum="0" Maximum="1"/>
        <UI:Slider VerticalAlignment="Top" Margin="10,23,10,0" x:Name="Delay" ValueChanged="Delay_OnValueChanged" Minimum="0" Maximum="1"/>
        <UI:Slider VerticalAlignment="Top" Margin="10,46,10,0" x:Name="Feedback" ValueChanged="Feedback_OnValueChanged" Minimum="0" Maximum="1"/>
        <UI:Slider VerticalAlignment="Top" Margin="10,69,10,0" x:Name="Pan" ValueChanged="Pan_OnValueChanged" Minimum="0" Maximum="1"/>
    </Grid>
</Page>
