<Window x:Class="JaebeMusicStudio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JaebeMusicStudio"
        xmlns:UI="clr-namespace:JaebeMusicStudio.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid Name="windowArea">
            <UI:ButtonPretty Height="20" Margin="0,10,10,0" VerticalAlignment="Top" FontFamily="/fonts/jms.ttf#jms" HorizontalAlignment="Left" Width="20" Icon="e800" x:Name="saveButton" Click="saveButton_Click"></UI:ButtonPretty>
            <UI:ButtonPretty Height="20" Margin="0,60,10,0" VerticalAlignment="Top" FontFamily="/fonts/jms.ttf#jms" HorizontalAlignment="Left" Width="20" Icon="e800" x:Name="openButton" Click="openButton_Click"></UI:ButtonPretty>
        </Grid>
    </Grid>
</Window>
